        <selectionEntry id="d4a5-bf90-b6cd-5954" name="Poción de curación" hidden="false" collective="false" import="true" type="upgrade">
          <constraints>
            <constraint field="selections" scope="roster" value="1.0" percentValue="false" shared="true" includeChildSelections="false" includeChildForces="false" id="5a87-3523-5650-370f" type="max"/>
          </constraints>
          <profiles>
            <profile id="29fd-4fcf-e9cd-a9b8" name="Poción de curación" hidden="false" typeId="0912-c473-dc44-71c2" typeName="Objetos mágicos">
              <characteristics>
                <characteristic name="Propiedades mágicas" typeId="e33b-a1fc-9f43-c88d">Un solo uso. El personaje recupera tantas heridas como tenía al inicio de la partida. Obviamente, no puede usarlo si está muerto.</characteristic>
              </characteristics>
            </profile>
          </profiles>
          <costs>
            <cost name="puntos" typeId="53ea-00f4-9046-81ff" value="50.0"/>
