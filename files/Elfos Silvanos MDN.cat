      </constraints>
      <selectionEntries>
        <selectionEntry id="2cf9-13fa-c3d2-5ae0" name="Estandarte de guerra" hidden="false" collective="false" import="true" type="upgrade">
          <constraints>
            <constraint field="selections" scope="roster" value="1.0" percentValue="false" shared="true" includeChildSelections="true" includeChildForces="false" id="af3b-cfcb-42ca-9fde" type="max"/>
          </constraints>
          <profiles>
