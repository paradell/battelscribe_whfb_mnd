            <constraint field="selections" scope="roster" value="1.0" percentValue="false" shared="true" includeChildSelections="true" includeChildForces="false" id="5952-4192-bd7f-0c5a" type="max"/>
          </constraints>
          <profiles>
            <profile id="72ea-0def-6ff3-3bc6" name="Faoghir, el estandarte del otoño" hidden="false" typeId="0912-c473-dc44-71c2" typeName="Objetos mágicos">
              <characteristics>
