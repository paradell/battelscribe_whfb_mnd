        <selectionEntry id="c4c3-fb36-d63f-52d2" name="Estandarte real de Ariel" hidden="false" collective="false" import="true" type="upgrade">
          <constraints>
            <constraint field="selections" scope="roster" value="1.0" percentValue="false" shared="true" includeChildSelections="true" includeChildForces="false" id="652d-e959-6284-1009" type="max"/>
