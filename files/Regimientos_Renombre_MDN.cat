    <selectionEntry id="a8ed-8c84-c5d6-eb17" name="Caballeros de la Llama Purificadora (Mercenarios e Imperio)(Nunca junto a Perros del Desierto o Comando Mortal)" hidden="false" collective="false" import="true" type="unit">
