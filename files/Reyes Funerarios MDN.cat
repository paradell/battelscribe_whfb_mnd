      <selectionEntries>
        <selectionEntry id="8770-06fe-441b-94df" name="Khepra azul" hidden="false" collective="false" import="true" type="upgrade">
          <constraints>
            <constraint field="selections" scope="roster" value="1.0" percentValue="false" shared="true" includeChildSelections="true" includeChildForces="false" id="fb9a-77fa-0b96-95b7" type="max"/>
